generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid())
  first_name       String
  last_name   String
  email   String @unique
  username   String @unique
  password   String  
  active Boolean @default(true)
  created_at DateTime @default(now())
  updated_at DateTime @default(now())


  
  @@map("users")
}

